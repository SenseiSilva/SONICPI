# Stranger Things Theme
# SHEET MUSIC - https://www.musicnotes.com/images/productimages/mtd/MN0179501.gif
# BPM - https://songbpm.com/stranger-things?q=Stranger%20Things

use_bpm 480
use_synth :saw

with_fx :distortion do
  
  loop do
    play :c4
    sleep 1
    play :e4
    sleep 1
    play :g4
    sleep 1
    play :b4
    sleep 1
    play :c5
    sleep 1
    play :b4
    sleep 1
    play :g4
    sleep 1
    play :e4
    sleep 1
  end
end

